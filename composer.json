{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "barryvdh/laravel-translation-manager": "^0.5.7",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "laracasts/flash": "^3.0",
        "laracasts/presenter": "^0.2.2",
        "laravel/framework": "^6.2",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.1",
        "laravelcollective/html": "^6.0",
        "matthiasmullie/minify": "^1.3",
        "orangehill/iseed": "^2.6",
        "snapappointments/bootstrap-select": "^1.13",
        "stichoza/google-translate-php": "^4.0",
        "tightenco/ziggy": "^0.8.1",
        "twbs/bootstrap": "^4.3",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fruitcake/laravel-telescope-toolbar": "^1.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "tanmuhittin/laravel-google-translate": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan vendor:publish --tag=public --force",
            "php artisan telescope:publish",
            "mkdir -p public/vendor/bootstrap-select",
            "cp -R vendor/snapappointments/bootstrap-select/dist/js/ public/vendor/bootstrap-select/js/",
            "cp -R vendor/snapappointments/bootstrap-select/dist/css/ public/vendor/bootstrap-select/css/"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=public --force",
            "php artisan telescope:publish",
            "mkdir -p public/vendor/bootstrap-select",
            "cp -R vendor/snapappointments/bootstrap-select/dist/js/ public/vendor/bootstrap-select/js/",
            "cp -R vendor/snapappointments/bootstrap-select/dist/css/ public/vendor/bootstrap-select/css/"
        ]
    }
}

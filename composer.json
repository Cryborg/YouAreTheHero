{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "barryvdh/laravel-translation-manager": "^0.5.7",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "laracasts/flash": "^3.0",
        "laracasts/presenter": "^0.2.2",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "^2.2",
        "snapappointments/bootstrap-select": "^1.13",
        "spatie/laravel-activitylog": "^3.14",
        "tightenco/ziggy": "^0.8",
        "twbs/bootstrap": "^4.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.1",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "orangehill/iseed": "^2.6",
        "phpunit/phpunit": "^8.5",
        "tanmuhittin/laravel-google-translate": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "php artisan ziggy:generate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan vendor:publish --tag=public --force",
            "php artisan telescope:publish",
            "php artisan ziggy:generate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N",
            "mkdir -p public/vendor/bootstrap-select",
            "cp -R vendor/snapappointments/bootstrap-select/dist/js/ public/vendor/bootstrap-select/js/",
            "cp -R vendor/snapappointments/bootstrap-select/dist/css/ public/vendor/bootstrap-select/css/"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=public --force",
            "php artisan telescope:publish",
            "php artisan ziggy:generate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N",
            "mkdir -p public/vendor/bootstrap-select",
            "cp -R vendor/snapappointments/bootstrap-select/dist/js/ public/vendor/bootstrap-select/js/",
            "cp -R vendor/snapappointments/bootstrap-select/dist/css/ public/vendor/bootstrap-select/css/"
        ]
    }
}
